<li class="folder-info"  data-id=<%= folder.id %> data-name='<%= folder.name %>' data-sitemap-count=<%= folder.sitemaps.size %>>
  <!-- <%#= link_to(image_tag('cross.png'), folder_path(folder), method: :delete, remote: true, class: "delete-folder") %> -->
  <a class='delete-folder'><%= image_tag('cross.png') %></a>
  <div href="javascript:void(0);" class='folder-info-block' data-name='<%= folder.name %>'>
    <span class="icon-folder-light"></span>
    <span class="folder-name">
      <span class='folder-name-text'><%= folder.name %></span>
      <input type='text' class='form-control folder-name-field hidden' data-id=<%= folder.id %> data-name='<%= folder.name %>' value='<%= folder.name %>'>
    </span>
    <span class="folder-items"><%= folder.sitemaps.size %></span>
  </div>
</li>
